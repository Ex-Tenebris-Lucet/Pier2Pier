{
  "name": "pier2pier",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "scripts": {
    "start": "concurrently -k \"cross-env BROWSER=none react-scripts start\" \"npx electron .\"",
    "build": "react-scripts build",
    "postinstall": "electron-builder install-app-deps && electron-rebuild",
    "rebuild": "electron-rebuild"
  },
  "dependencies": {
    "better-sqlite3": "^8.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
